{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache  --continue",
    "dev:dns": "turbo run dev --no-cache --filter=dns...",
    "dev:transfer": "turbo run dev --no-cache --filter=transfer...",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
